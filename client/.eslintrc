{
  "parser": "babel-eslint",
  "plugins": ["react"],
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "strict": 0,
    "quotes": 0,
    "no-unused-vars": 0,
    "camelcase": 0,
    "no-underscore-dangle": 0,
    "semi": [
      2,
      "never"
    ],
    "no-use-before-define": 0,
    "react/prefer-stateless-function": 0,
    "comma-dangle": 0
  },
  "globals": {
    "google": true
  }
}
